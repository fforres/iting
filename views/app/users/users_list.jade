extends ../../layout
block content
	.row
		.col-xs-11.col-sm-11.col-centered#userslist
			include ../../snipets/errors
			each user,i in users
				.col-xs-12.user(data-userid="#{user.id}")
					.col-sm-8.info
						div.data
							span.nombre #{user.username}
							span.email #{user.email}
						div.acciones
							a(href="#")
								i.fa.fa-fw.fa-ellipsis-h  
					.col-sm-4.roles
		